            <b>bankAccounts</b>
        </field-exclude>

        <field-exclude>
            <a>relations</a>
            <b>relations</b>
        </field-exclude>

        <field-exclude>
            <a>translates</a>
            <b>translates</b>
        </field-exclude>
    </mapping>
</mappings>